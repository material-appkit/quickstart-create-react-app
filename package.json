  "keywords": [],
  "description": "Material-AppKit PWA Starter",
  "private": true,
  "homepage": "/quickstart-create-react-app",
  "dependencies": {
    "@material-ui/core": "^4.11.2",
    "@material-ui/icons": "^4.11.2",
    "@material-ui/lab": "^4.0.0-alpha.57",
    "dayjs": "^1.9.7",
    "notistack": "^1.0.2",
    "prop-types": "^15.7.2",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-helmet-async": "^1.0.7",
    "react-intl-universal": "^2.4.2",
    "react-router-dom": "^5.2.0"
  },
  "devDependencies": {
    "dotenv-cli": "^4.0.0",
    "gh-pages": "^3.1.0",
    "lodash.debounce": "^4.0.8",
    "lodash.isequal": "^4.5.0",
    "query-string": "^6.13.7",
    "react-scripts": "^4.0.1",
    "source-map-explorer": "^2.5.1"
  },
  "scripts": {
    "start": "react-scripts start",
    "start-secure": "HTTPS=true react-scripts start",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "build": "react-scripts build",
    "build-dev": "dotenv -e .env.development react-scripts build",
    "build-local": "dotenv -e .env.development.local react-scripts build",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build",
    "test": "react-scripts test --env=jsdom",
    "eject": "react-scripts eject"
  },
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
